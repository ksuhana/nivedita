<body>
    <canvas id="mycanvas" widith="300" height="300"></canvas>
    <script>
    var c=document.getElementById("mycanvas");
    var ctx=c.getcontext("2d");
    <br><br>

     //rectangle
    ctx.fillstyle="red";
    ctx.fillrect(75,75,150,150);
    </script>
